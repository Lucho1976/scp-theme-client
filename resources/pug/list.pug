list.panel.panel-default(
  list="vm.list"
)
  .panel-heading
    .btn-group-xs.pull-right
      block btns
    h3.panel-title
      block title
  block above-table
  table.table.table-bordered.table-striped.table-condensed.table-hover.table-no-edge-border
    thead
      tr
        block headings-pre
        block headings
    tbody
      tr(
        ng-repeat="row in vm.list.items track by row.id"
        ng-click="row.checked = !row.checked"
      )
        block columns-pre
        block columns
  list-footer
